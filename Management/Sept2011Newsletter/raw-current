&lt;script language=&quot;javascript&quot; type=&quot;text/javascript&quot;&gt;
&lt;!--
function openWindow(url, name, width, height, resizable, scrollbars, status, menubar, toolbar, locationbar)
{
	var options = &quot;width=&quot; + width + &quot;,height=&quot; + height + &quot;,&quot;;
	options += &quot;resizable=&quot; + resizable + &quot;,scrollbars=&quot; + scrollbars + &quot;,status=&quot; + status + &quot;,&quot;;
	options += &quot;menubar=&quot; + menubar + &quot;,toolbar=&quot; + toolbar + &quot;,location=&quot; + locationbar + &quot;,directories=no&quot;;
	var newWin = window.open(url, name, options);
	newWin.focus();
}


//--&gt;
&lt;/script&gt;

