    document.getElementById(&#39;SecuritySearch&#39;).setAttribute(&quot;class&quot;, &quot;hidden&quot;);
};
document.getElementById(&#39;whichweb&#39;).addEventListener(&#39;change&#39;, toggleVisibility);
function toggleVisibility(){
	var men1 = document.getElementById(&quot;browseOrSearch&quot;)
	var men2 = document.getElementById(&quot;whichweb&quot;)
if (men1.value==1 &amp;&amp; men2.value==5){
 document.getElementById(&#39;OperationsSearch&#39;).setAttribute(&quot;class&quot;, &quot;visible&quot;);
 document.getElementById(&#39;SecuritySearch&#39;).setAttribute(&quot;class&quot;, &quot;hidden&quot;);
}
else if (men1.value==1 &amp;&amp; men2.value==4){
 document.getElementById(&#39;OperationsSearch&#39;).setAttribute(&quot;class&quot;, &quot;hidden&quot;);
 document.getElementById(&#39;SecuritySearch&#39;).setAttribute(&quot;class&quot;, &quot;visible&quot;);
}
else if (men1.value==2 &amp;&amp; men2.value==5){
 window.open (&quot;https://twiki.grid.iu.edu/bin/view/Operations&quot;,&quot;_self&quot;);
 document.getElementById(&#39;OperationsSearch&#39;).setAttribute(&quot;class&quot;, &quot;hidden&quot;);
 document.getElementById(&#39;SecuritySearch&#39;).setAttribute(&quot;class&quot;, &quot;hidden&quot;);
}
else if (men1.value==2 &amp;&amp; men2.value==4){
 window.open (&quot;https://twiki.grid.iu.edu/bin/view/Security&quot;,&quot;_self&quot;);
 document.getElementById(&#39;OperationsSearch&#39;).setAttribute(&quot;class&quot;, &quot;hidden&quot;);
 document.getElementById(&#39;SecuritySearch&#39;).setAttribute(&quot;class&quot;, &quot;hidden&quot;);
}
else {
document.getElementById(&#39;OperationsSearch&#39;).setAttribute(&quot;class&quot;, &quot;hidden&quot;);
document.getElementById(&#39;SecuritySearch&#39;).setAttribute(&quot;class&quot;, &quot;hidden&quot;);
	};
}
