        description=&#39;&#39;
        vomsServer=&#39;sbgrid&#39;
        matchFQAN=&#39;vo&#39;
        acceptProxyWithoutFQAN=&#39;true&#39;
        voGroup=&#39;/SBGrid&#39;/&gt;

&lt;vomsUserGroup
