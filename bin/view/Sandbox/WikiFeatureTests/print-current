<h1><a name="Welcome_to_the_Open_Science_Grid"></a> Welcome to the <span>Open Science Grid</span> Documentation Hub </h1>
<h2 class="twikinetRoundedAttachments"><span class="twikinetHeader"><a name="What_can_we_help_you_find_today"></a> What can we help you find today? </span></h2>
<p>
I would like to... 
<select id = "browseOrSearch" onchange="reset2()">
  <option value="0"> </option>
  <option value="1">search</option>
  <option value="2">browse</option>
</select>
the documentation intended for 
<select id = "whichweb">
  <option value="3"> </option>
  <option value="4">security</option>
  <option value="5">operations</option>
</select>
</p>
<script>
function reset2(){
	document.getElementById('whichweb').value = "";
	document.getElementById('OperationsSearch').setAttribute("class", "hidden");
    document.getElementById('SecuritySearch').setAttribute("class", "hidden");
};
document.getElementById('whichweb').addEventListener('change', toggleVisibility);
function toggleVisibility(){
	var men1 = document.getElementById("browseOrSearch")
	var men2 = document.getElementById("whichweb")
if (men1.value==1 && men2.value==5){
 document.getElementById('OperationsSearch').setAttribute("class", "visible");
 document.getElementById('SecuritySearch').setAttribute("class", "hidden");
}
else if (men1.value==1 && men2.value==4){
 document.getElementById('OperationsSearch').setAttribute("class", "hidden");
 document.getElementById('SecuritySearch').setAttribute("class", "visible");
}
else if (men1.value==2 && men2.value==5){
 window.open ("https://twiki.grid.iu.edu/bin/view/Operations","_self");
 document.getElementById('OperationsSearch').setAttribute("class", "hidden");
 document.getElementById('SecuritySearch').setAttribute("class", "hidden");
}
else if (men1.value==2 && men2.value==4){
 window.open ("https://twiki.grid.iu.edu/bin/view/Security","_self");
 document.getElementById('OperationsSearch').setAttribute("class", "hidden");
 document.getElementById('SecuritySearch').setAttribute("class", "hidden");
}
else {
document.getElementById('OperationsSearch').setAttribute("class", "hidden");
document.getElementById('SecuritySearch').setAttribute("class", "hidden");
	};
}
</script>
</p>
<form name="quickSearchForm" id="SecuritySearch" class="hidden" action="/bin/view/Security/WebSearch"><input type="text" class="twikiInputField patternFormFieldDefaultColor" name="search" value="Specific Web Search (Currently Set To Security)" size="100" onfocus="clearDefaultandCSS(this);" onblur="setDefaultText(this);" /></form>
