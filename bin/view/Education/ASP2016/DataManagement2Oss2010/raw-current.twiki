Error = test.$(Cluster).$(Process).err
transfer_executable = true
transfer_input_files = blast.sh,yeast.query1
WhenToTransferOutput = ON_EXIT_OR_EVICT
transfer_output_files = blast_results.$(Cluster).$(Process)
should_transfer_files   = YES
Arguments =  %SURL%/%VO%/blast/db-1 yeast.aa yeast.query1  blast_results.$(Cluster).$(Process)
queue
